{
  "name": "",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "generate": "node ./blueprints/generate.js",
    "destroy": "node ./blueprints/destroy.js",
    "build": "cross-env NODE_ENV=production webpack -p",
    "dev": "cross-env NODE_ENV=development node webpack-dev-server.js",
    "test": "cross-env NODE_ENV=test mocha --exit --require babel-core/register init.test.js \"src/**/*.test.{js,jsx}\"",
    "test:watch": "cross-env NODE_ENV=test mocha --watch --exit --require babel-core/register init.test.js \"src/**/*.test.{js,jsx}\"",
    "test:coverage": "cross-env NODE_ENV=test nyc --all npm run test",
    "lint": "eslint -c .eslintrc.json src/**/*.{js,jsx} \"blueprints/**/*.{js,jsx,scss}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,scss}\" \"blueprints/**/*.{js,jsx,scss}\"",
    "format:watch": "onchange \"src/**/*.{js,jsx,scss}\" -- prettier --write {{changed}}",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,scss}\" \"blueprints/**/*.{js,jsx,scss}\"",
    "eslint-prettier-compatibility-check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "btoa": "^1.1.2",
    "chai": "^4.1.2",
    "chai-diff": "^1.0.1",
    "chai-enzyme": "^0.8.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "dotenv": "^5.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-mocha": "^4.12.1",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.0.6",
    "jsdom": "^11.6.2",
    "mocha": "^5.0.4",
    "mock-css-modules": "^1.0.0",
    "node-sass": "^4.7.2",
    "nyc": "^11.6.0",
    "onchange": "^3.3.0",
    "postcss-loader": "^2.1.1",
    "prettier": "^1.11.1",
    "proxyquire": "^2.0.0",
    "redux-logger": "^3.0.6",
    "sass-loader": "^6.0.7",
    "sinon": "^4.4.6",
    "sinon-chai": "^3.0.0",
    "snap-shot-it": "^4.1.4",
    "style-loader": "^0.20.3",
    "webpack": "^4.1.1",
    "webpack-dev-server": "^3.1.1",
    "write-file-webpack-plugin": "^4.2.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "redux": "^3.7.2"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 98,
    "include": [
      "src"
    ],
    "extension": [
      ".jsx"
    ],
    "exclude": [
      "assets",
      "build",
      "blueprints",
      "**/*.test.js",
      "**/*.test.jsx"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "all": true,
    "sourceMap": false,
    "instrument": false
  }
}
